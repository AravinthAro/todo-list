<div class="todo-list-action-center">
  <button onclick={{action addNewRow}} class="todo-list-btn todo-list-btn--add">
    Add item
  </button>
  {{!-- <button class="todo-list-btn todo-list-btn--delete">
    Delete item
  </button> --}}
</div>
<div class="todo-list-main-body todo-list-border">
  {{#if mainData}}
    {{#each mainData as |row index|}}
      {{todo-list-item data=row index=index isOpen=(util-equal index openedItemIndex)}}
    {{/each}}
  {{else}}
    {{todo-list-loader count=10}}
  {{/if}}
    
</div>